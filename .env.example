# VALKEY Configuration. You should be able to leave these as-is for local development.
VALKEY_HOST=valkey
VALKEY_PORT=6379
VALKEY_PASSWORD=change_in_production
VALKEY_USE_SSL=false

# API key used for authenticating API requests. Change this in production!
API_KEY=your_api_key_here_change_in_production

# JWT secret for signing JSON Web Tokens. Set it to a strong, random value in production!
JWT_SECRET=your_jwt_secret_change_in_production

# Paths for build storage. You should be able to leave these as-is unless you have specific requirements.
BUILD_INGEST_PATH=/builds
TEMP_BUILD_PATH=/tmp/builds

# Default admin credentials. Change these in production!
DEFAULT_USERNAME=admin
DEFAULT_PASSWORD=change_this_password_in_production

# Steam configuration for Steam channel support (optional)
STEAM_USERNAME=your_steam_username

# Notification Webhooks. Comment out or remove any you do not wish to use.
# Slack webhook URL for build notifications
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
# Discord webhook URL for build notifications
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR/WEBHOOK/URL
